#
# ~/.bash_profile
#

# source bashrc if it exists
[[ -f ~/.bashrc ]] && . ~/.bashrc

echo helllo
export TERM='xterm-256color'
export EDITOR='vim'

export CLICOLOR=1
export LSCOLORS=GxFxCxDxBxegedabagaced

# set PATH so it includes user's private bin if it exists
if [ -d ~/bin ] ; then
    PATH=~/bin:"${PATH}"
fi
export PATH
